					<xs:documentation>Frais effectifs non compris dans les prestations soumises à l'impôt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fraisForfaitairesNonInclus" type="xs:integer" minOccurs="0">
